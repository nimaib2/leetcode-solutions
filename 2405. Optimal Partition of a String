class Solution {
    public int partitionString(String s) {
        String str = s.substring(0,1);
        int combos=1;
        for(int i=1; i<s.length(); i++){
            if(str.indexOf((int)s.charAt(i))==-1){
                str+=s.charAt(i);
            } else {
                str="";
                str+=s.charAt(i);
                combos++;
            }
        }
        return combos;
    }
}
